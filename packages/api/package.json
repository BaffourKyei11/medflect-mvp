{
  "name": "@medflect/api",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:build\" \"npm:dev:start\"",
    "dev:build": "tsc -w -p tsconfig.json",
    "dev:start": "nodemon --watch dist --ext js --exec \"node dist/index.js\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "worker": "node dist/index.js",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_auth",
    "chain:env": "node scripts/load-local-chain.js",
    "chain:logs": "node ../../blockchain/scripts/printAccessLogs.js --from 2000",
    "chain:e2e": "node scripts/e2e-consent-audit.js",
    "chain:set-operator": "node ../../blockchain/scripts/setOperator.js"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "prom-client": "^15.1.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prisma": "^5.17.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
